{
  "name": "max/max",
  "license": "Apache-2.0",
  "description": "一款支持swoole, workerman, FPM环境的框架的组件化的轻量`PHP`框架。",
  "homepage": "https://github.com/marxphp/max",
  "keywords": [
    "maxphp",
    "flexible",
    "php framework"
  ],
  "autoload": {
    "psr-4": {
      "Max\\": "src/framework/src",
      "Max\\Aop\\": "src/aop/src",
      "Max\\Cache\\": "src/cache/src",
      "Max\\Config\\": "src/config/src",
      "Max\\Database\\": "src/database/src",
      "Max\\Di\\": "src/di/src",
      "Max\\Event\\": "src/event/src",
      "Max\\JsonRpc\\": "src/json-rpc/src",
      "Max\\Pool\\": "src/pool/src",
      "Max\\Macro\\": "src/macro/src",
      "Max\\Http\\Message\\": "src/http-message/src",
      "Max\\Http\\Server\\": "src/http-server/src",
      "Max\\Resource\\": "src/resource/src",
      "Max\\Routing\\": "src/routing/src",
      "Max\\Session\\": "src/session/src",
      "Max\\Utils\\": "src/utils/src",
      "Max\\Validation\\": "src/validation/src",
      "Max\\View\\": "src/view/src",
      "Max\\VarDumper\\": "src/var-dumper/src"
    },
    "files": [
      "src/di/src/helpers.php",
      "src/utils/src/helpers.php",
      "src/var-dumper/src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "chengyao",
      "email": "987861463@qq.com"
    }
  ],
  "replace": {
    "max/framework": "*",
    "max/aop": "*",
    "max/cache": "*",
    "max/config": "*",
    "max/database": "*",
    "max/di": "*",
    "max/event": "*",
    "max/pool": "*",
    "max/json-rpc": "*",
    "max/http-message": "*",
    "max/http-server": "*",
    "max/resource": "*",
    "max/routing": "*",
    "max/session": "*",
    "max/macro": "*",
    "max/utils": "*",
    "max/validation": "*",
    "max/view": "*",
    "max/var-dumper": "*"
  },
  "require": {
    "php": "^8.0",
    "psr/container": "^1.0|^2.0",
    "psr/simple-cache": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.11",
    "paulxu-cn/event-ide-helper": "^0.1.0",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.7"
  },
  "scripts": {
    "test": "phpunit --prepend tests/bootstrap.php -c phpunit.xml --colors=always",
    "cs-fix": "php-cs-fixer fix $1",
    "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon $1"
  }
}
