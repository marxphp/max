{
  "name": "max/max",
  "description": "一款基于组件的轻量PHP框架。",
  "homepage": "https://github.com/topyao/max",
  "keywords": [
    "maxphp",
    "flexible",
    "php framework"
  ],
  "type": "project",
  "authors": [
    {
      "name": "chengyao",
      "email": "bigyao@139.com"
    }
  ],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "require": {
    "php": ">=7.2.0",
    "max/framework": "1.0.1",
    "max/cache": "1.0.1",
    "max/database": "1.0.1",
    "max/console": "1.0.1",
    "max/view": "1.0.1",
    "max/validator": "1.0.1",
    "max/redis": "1.0.1",
    "max/filesystem": "0.0.1",
    "max/http": "1.0.1",
    "max/log": "0.0.1"
  },
  "require-dev": {
    "symfony/var-dumper": "^5.3",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php max vendor:publish"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.example.env', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
