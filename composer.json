{
  "name": "max/max",
  "description": "一款基于组件的轻量PHP框架。",
  "homepage": "https://github.com/topyao/max",
  "keywords": ["maxphp", "flexible", "php framework"],
  "type": "project",
  "authors": [
    {
      "name": "chengyao",
      "email": "bigyao@139.com"
    }
  ],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/common.php"
    ]
  },
  "require": {
    "php": ">=7.2.0",
    "max/captcha": "dev-master",
    "max/framework": "dev-master",
    "max/cache": "dev-master",
    "max/database": "dev-master",
    "max/container": "dev-master",
    "max/view": "dev-master",
    "max/validator": "dev-master",
    "max/redis": "dev-master",
    "max/console": "dev-master",
    "max/filesystem": "dev-master",
    "max/workerman": "dev-master",
    "max/swoole": "dev-master",
    "max/http": "dev-master",
    "max/env": "dev-master",
    "max/helper": "dev-master",
    "max/log": "dev-master"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php max vendor:publish"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
