{
  "name": "max/max",
  "description": "一个简单易用的轻量PHP框架。",
  "keywords": [
    "MAXPHP",
    "轻量",
    "简单"
  ],
  "type": "project",
  "authors": [
    {
      "name": "chengyao",
      "email": "bigyao@139.com"
    }
  ],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/common.php"
    ]
  },
  "require": {
    "php": ">=7.2.0"
  },
  "require-dev": {
    "max/framework": "1.0.*",
    "max/cache": "1.0.*",
    "max/database": "1.0.*",
    "max/view": "1.0.*",
    "max/validator": "1.0.*",
    "max/console": "1.0.*",
    "max/filesystem": "1.0.*"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php max vendor:publish"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
