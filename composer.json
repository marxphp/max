{
    "name": "max/max",
    "license": "Apache-2.0",
    "description": "一款支持swoole, workerman, FPM环境的框架的组件化的轻量`PHP`框架。",
    "homepage": "https://github.com/topyao/max",
    "keywords": [
        "maxphp",
        "flexible",
        "php framework"
    ],
    "autoload": {
        "psr-4": {
            "Max\\Aop\\": "src/aop/src",
            "Max\\Cache\\": "src/cache/src",
            "Max\\Config\\": "src/config/src",
            "Max\\Context\\": "src/context/src",
            "Max\\Database\\": "src/database/src",
            "Max\\Di\\": "src/di/src",
            "Max\\Event\\": "src/event/src",
            "Max\\Framework\\": "src/framework/src",
            "Max\\Http\\Server\\": "src/http-server/src",
            "Max\\Http\\Message\\": "src/http-message/src",
            "Max\\Queue\\": "src/queue/src",
            "Max\\JWT\\": "src/jwt/src",
            "Max\\Routing\\": "src/routing/src",
            "Max\\Session\\": "src/session/src",
            "Max\\Utils\\": "src/utils/src",
            "Max\\Validator\\": "src/validator/src",
            "Max\\View\\": "src/view/src"
        },
        "files": [
            "src/di/src/helpers.php",
            "src/utils/src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "chengyao",
            "email": "987861463@qq.com"
        }
    ],
    "replace": {
        "max/cache": "*",
        "max/config": "*",
        "max/database": "*",
        "max/di": "*",
        "max/aop": "*",
        "max/event": "*",
        "max/framework": "*",
        "max/http-message": "*",
        "max/http-server": "*",
        "max/log": "*",
        "max/queue": "*",
        "max/routing": "*",
        "max/session": "*",
        "max/utils": "*",
        "max/validator": "*",
        "max/view": "*"
    },
    "require": {
        "php": "^8.0",
        "psr/container": "^2.0",
        "psr/simple-cache": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/log": "^1.0",
        "monolog/monolog": "^2.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.7"
    },
    "scripts": {
        "test": "co-phpunit --prepend test/bootstrap.php -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src"
    }
}
